// Arup Guha
// 2/27/08
// Example of a heap data structure implementation for COP 3502

#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <string.h>


struct Node {
	int id;
	char record[1014];
};
typedef struct Node Node;
struct heapStruct {

	Node *heaparray;
	int capacity;
	int size;
};
struct arrayNode {
	int key;
	Node node;
};
typedef struct arrayNode arrayNode;



struct heapStruct *initHeap();
void shiftDown(struct heapStruct *h, int index);
void shiftUp(struct heapStruct *h, int index);
void insert(struct heapStruct *h, Node value);
Node deleteHeap(struct heapStruct *h);
void swap(struct heapStruct *h, int index1, int index2);
void freeHeap(struct heapStruct *h);

void arrayinsert(arrayNode a[],int size,arrayNode new ) ;
arrayNode arrayDelete(arrayNode a[],int size) ;



int main(int argc,char **argv) {
	

	int buffersize = 0 ;
	int pagesize = 0;
	if(argc == 5 ) 
	{ 
		char * temp = argv[3] ;
		int i= 0; 
		while(temp[i] != '=') //esittire kadar ilerliyo
		{ 
			i++; 
		} 
		temp = (char*)&temp[i+1]; 
		buffersize = atoi(temp); //buffersize aliniyo
		
		temp = argv[4];
		while(temp[i] != '=') 
		{ 
			i++; 
		} 
		temp = (char*)&temp[i+1]; 
		pagesize = atoi(temp) * 1024; //pagesize aliniyo
		
		struct heapStruct *heap1;
		struct heapStruct *heap2;
		heap1 = initHeap(pagesize/1024);// heapler yaratiliyo pagesize/1024 kadar capasity e sahip
		heap2 = initHeap(pagesize/1024);
		
		FILE *fp = fopen(argv[1],"r"); // kullanicinin girdigi dosyayi read formatinda aciyorum
		if(!fp) {
			
			printf("Could not open file!");//acamazsam cikiyorum
			return 0;
		}
		
		int filenum=1; // dosyalarimi isimlendirmek icin kullandigim degisken dosyalarim file_0_1 gibi adlandiriliyo 
		char filename[20];//olusan dosyalarimin adi    		// 0 derinlik 1 o derinliktekacinci dosya oldugu
		while(!feof(fp) && heap1->size != heap1->capacity){ //heap1 im dolana kadar dosyadan okuyorum
			Node readData ;
			fscanf(fp,"%d,",&readData.id);//virgule kadar id
			fgets(readData.record,1014,fp);//1014 uzunlugunda record
			if (!feof(fp)) {
				insert(heap1,readData); //dosyanin sonu degilse heap1 e ekliyorum
			}
		}
		sprintf(filename,"file_0_%d.txt",filenum); 
		FILE *writeFile = fopen(filename, "w");
		int lastKey = 0;
		while(!feof(fp)){

			if(!fp) {
				printf("Could not open file");
				return 0;
			}
			if(heap1->size == 0)
			{
				filenum++;
				sprintf(filename,"file_0_%d.txt",filenum);
				fclose(writeFile);
				writeFile = fopen(filename, "w");
				while(heap2->size != 0) {
					insert(heap1,deleteHeap(heap2));
				}
				lastKey = 0;

			}
			Node writeRecord = deleteHeap(heap1);
			fprintf(writeFile, "%8d, %s\n",writeRecord.id, writeRecord.record);
			lastKey = writeRecord.id;//key g√ºncelleme
			Node newRecord;
			fscanf(fp,"%8d,",&newRecord.id);
			fgets(newRecord.record,1014,fp);
			if (!feof(fp)) {
				if(lastKey< newRecord.id){
					insert(heap1,newRecord);
				} else{
					insert(heap2,newRecord);
				}
			}

		}
		while(heap1->size != 0) {
			Node writeRecord = deleteHeap(heap1);
			fprintf(writeFile, "%8d, %s\n",writeRecord.id, writeRecord.record);

		}
		
		if (heap2->size != 0) {
			filenum++;
			fclose(writeFile);
			sprintf(filename,"file_0_%d.txt",filenum);
			FILE *writeFile = fopen(filename, "w");
			while(heap2->size != 0) {
				Node writeRecord = deleteHeap(heap2);
				fprintf(writeFile, "%8d, %s\n",writeRecord.id, writeRecord.record);
			}

		}

		fclose(writeFile);
	
		int deep =0;
		while(filenum > 1) {
			int divideNum = filenum/(buffersize-1); // 1 tanesi output buffer divide num toplam dosya sayimi kac parcada merge edecegim
			if (filenum%(buffersize-1) != 0 ) {
				divideNum++;
			}
			int i = 0;
			for (i = 1;  i <= divideNum;  i++) {
				int fileArraySize = 0;
				if (i != divideNum || filenum%(buffersize-1) == 0) {
					fileArraySize = buffersize-1;
				}else {
					fileArraySize = filenum%(buffersize-1);
				}
				sprintf(filename,"file_%d_%d.txt",deep+1,i);
				writeFile = fopen(filename,"w");
				FILE  *readFile[fileArraySize];
				arrayNode mergeArray[fileArraySize] ;
				int j=0 ;
				int arraySize = 0;

				for (j = 1; j <= fileArraySize; j++) {

					sprintf(filename,"file_%d_%d.txt",deep,j+(i-1)*(buffersize-1));
					readFile[j-1] = fopen(filename,"r");
					if (!feof(readFile[j-1])) {
						arrayNode newrecord;
						fscanf(readFile[j-1],"%8d",&newrecord.node.id);
						fgets(newrecord.node.record,1014,readFile[j-1]);
						newrecord.key = j-1;
						if (!feof(readFile[j-1])) {
							arrayinsert(mergeArray,arraySize,newrecord);
							arraySize++;
						}
					}


				}
				while(arraySize!=0){
					arrayNode temp = arrayDelete(mergeArray,arraySize);
					arraySize--;
					fprintf(writeFile, "%d%s\n",temp.node.id, temp.node.record);
					int filepoint = temp.key;
					if (!feof(readFile[filepoint])) {
						arrayNode newrecord;
						fscanf(readFile[filepoint],"%8d",&newrecord.node.id);
						fgets(newrecord.node.record,1014,readFile[filepoint]);
						newrecord.key = filepoint;
						if (!feof(readFile[filepoint])) {
							arrayinsert(mergeArray,arraySize,newrecord);
							arraySize++;
						}
					}

				}
				fclose(writeFile);
				int k = 0;
				for (k = 0; k < fileArraySize; k++) {
					fclose(readFile[k]);
					sprintf(filename,"file_%d_%d.txt",deep,k+1+(i-1)*(buffersize-1));
					remove(filename);
				}
			}//for son for level
			deep++;
			filenum = divideNum-1;
		}//whileson
		sprintf(filename,"file_%d_1.txt",deep);
		rename(filename,argv[2]);
		printf("finish");
	}else {
		printf("EXAMPLE : unsorteddata.txt sorteddata.txt b=** p=** ");
	}
	return 0;

}

//////////////////////////Heap//////////////////////////
// Initialize an empty heap with a capacity of SI
struct heapStruct* initHeap(int capacity) {

	struct heapStruct* h;
	h = (struct heapStruct*)(malloc(sizeof(struct heapStruct)));
	h->capacity = capacity;
	h->heaparray = malloc(sizeof(Node)*(capacity));
	h->size = 0;
	return h;
}

void freeHeap(struct heapStruct *h) {
	free(h->heaparray);
	free(h);
}
void shiftDown(struct heapStruct *h, int index) {

	int minimum;
	if ((2*(index+1)) < h->size) {

		
		if(h->heaparray[2*index+1].id <  h->heaparray[2*(index+1)].id) {
			minimum = 2*index+1 ;
		} else {
			minimum = 2*(index+1);
		}
		
		if (h->heaparray[index].id > h->heaparray[minimum].id) {
			swap(h, index, minimum);
			shiftDown(h, minimum);
		}
	}
	else if (h->size > 2*index+1) {

		if (h->heaparray[index].id > h->heaparray[2*index+1].id)
			swap(h, index, 2*index+1);
	}
}
void shiftUp(struct heapStruct *h, int index) {

	if (index > 0) {

		int parent = (index-1)/2;
		if (h->heaparray[parent].id > h->heaparray[index].id) {

			swap(h, index, parent);
			shiftUp(h, parent);
		}
	}
}
void insert(struct heapStruct *h, Node value) {

	h->size++;
	h->heaparray[h->size-1] = value;
	shiftUp(h, h->size-1);
}
Node deleteHeap(struct heapStruct *h) {

	Node returnvalue;
	if (h->size != 0) {
		returnvalue = h->heaparray[0];
		h->heaparray[0] = h->heaparray[h->size-1];
		h->size--;
		shiftDown(h, 0);
	}
	return returnvalue;
}
void swap(struct heapStruct *h, int index1, int index2) {
	Node temp = h->heaparray[index1];
	h->heaparray[index1] = h->heaparray[index2];
	h->heaparray[index2] = temp;
}
////////////////////////HeapEND/////////////////////////

///////////////////////SortedArray//////////////////////
void arrayinsert(arrayNode a[],int size,arrayNode new ) {
	int i;
	for (i = size; i > 0; i--) {
		if (a[i-1].node.id> new.node.id) {
			a[i] = a[i-1];
		}else {
			break;
		}
	}
	a[i] = new;
}
arrayNode arrayDelete(arrayNode a[],int size) {
	arrayNode temp;
	temp = a[0];
	int i ;

	for (i = 1; i < size; i++) {
		a[i-1] = a[i];
	}
	return temp;

}
//////////////////////arrayEND//////////////////////////////
